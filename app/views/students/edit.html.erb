<%= form_with url: "/students/#{@student.id}", method: :patch, local: true do |form| %>
   <p><%= form.label :name %>
   <%= form.text_field :name %></p>
   <p><%= form.label :class_rank %>
   <%= form.text_field :class_rank %></p>
   <p><%= form.label :honor_roll %>
   <%= form.text_field :honor_roll %></p>
   <%= form.submit 'Update Student' %>
<% end %>